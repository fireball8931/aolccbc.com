{
  "name": "aolccbc.com",
  "description": "This is the source code for aolccbc.com",
  "version": "2.0.0",
  "scripts": {
    "build": "npm run make_programs_js && node ./generate_program_pages.js && npm run sitemap && parcel build",
    "check": "node validate.js",
    "dev": "npm run make_programs_js && node ./generate_program_pages.js && parcel",
    "debug": "npm run make_programs_js && node ./generate_program_pages.js && npm run sitemap && parcel build --log-level verbose",
    "live_debug": "npm run build_program_pages && parcel --log-level verbose --open",
    "sitemap": "npx sitemap-generator-cli -f static/sitemap.xml -d 5 -v -l aolccbc.com",
    "make_programs_js": "node ./scripts/compile_programs",
    "build_program_pages": "node ./generate_program_pages.js"
  },
  "source": "src/index.html",
  "targets": {
    "default": {
      "distDir": "./www"
    }
  },
  "engines": {
    "node": ">=19.8.0",
    "npm": ">=9.6.0",
    "posthtml": ">=0.16.6",
    "@parcel/transformer-jsonld": ">=2.7.0",
    "parcel": ">=2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireball8931/aolccbc.com.git"
  },
